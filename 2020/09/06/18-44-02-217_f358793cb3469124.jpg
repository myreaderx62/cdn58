var map;

function buildContentString(location) {
    var content = "<strong>City:</strong> " + location["city"] + "<br/>";
    for (var station_index = 0; station_index < location["station"].length; ++station_index) {
        var station = location["station"][station_index];
        content = content.concat("<strong>Station:</strong> " + station["id"] + "<br/><strong>Time:</strong> ");
        var time_count = station["times"].length;
        for (var i = 0; i < time_count - 1; ++i) {
            var time = station["times"][i];
            content = content.concat(time["day"] + " " + time["time"] + ", ");
        }
        var time = station["times"][time_count - 1];
        content = content.concat(time["day"] + " " + time["time"]);
        if (station_index < location["station"].length - 1) {
            content = content.concat("</br>");
        }
    }
    return content;
}

function initialize() {
    if (!jQuery("#map-canvas").length) {
        // Skip all of this for pages that don't have a map.
        return;
    }
    map = new google.maps.Map(document.getElementById('map-canvas'), {
        zoom: 3,
        center: {
            lat: 41,
            lng: -111
        }
    });
    jQuery.each(station_data_by_state, function(state, locations) {
        var state_header = jQuery("<tr></tr>");
        state_header.append(jQuery("<td colspan=4 style='font-size: 1em !important; vertical-align: middle !important; text-align: center !important; background-color: #DDDDDD;'><strong>" + state + "</strong></td>"));
        jQuery("#station_table").append(state_header);
        for (var station_index = 0; station_index < locations.length; station_index++) {
            (function() {
                var location = locations[station_index];
                var marker = new google.maps.Marker({
                    position: location["latlng"],
                    icon: "https://maps.gstatic.com/intl/en_us/mapfiles/markers2/measle.png",
                    map: map,
                });
                var infowindow = new google.maps.InfoWindow({
                    content: buildContentString(location),
                    position: location["latlng"]
                });
                google.maps.event.addListener(marker, 'click', function() {
                    infowindow.open(map, marker);
                });

                var station_row_span = 0;
                var time_row_span = [];
                for (var i = 0; i < location["station"].length; i++) {
                    station_row_span = station_row_span + location["station"][i]["times"].length;
                    time_row_span.push(location["station"][i]["times"].length);
                }
                for (var i = 0; i < location["station"].length; i++) {
                    var station = location["station"][i];
                    for (var j = 0; j < station["times"].length; j++) {
                        var time = station["times"][j];
                        var row = jQuery("<tr></tr>");
                        jQuery("#station_table").append(row);
                        if (i == 0 && j == 0) {
                            var cell = jQuery("<td style='font-size: 1em !important; vertical-align: middle !important;' rowspan=" + station_row_span + ">" + location["city"] + "</td>");
                            row.append(cell);
                        }
                        if (j == 0) {
                            var cell = jQuery("<td style='font-size: 1em !important; vertical-align: middle !important;' rowspan=" + time_row_span[i] + ">" + station["id"] + "</td>");
                            row.append(cell);
                        }
                        row.append(jQuery("<td style='font-size: 1em !important; vertical-align: middle !important;'>" + time["day"] + "</td>"));
                        row.append(jQuery("<td style='font-size: 1em !important; vertical-align: middle !important;'>" + time["time"] + "</td>"));
                    }
                }
            })();
        }
    });
    jQuery("#station_loading_row").remove();
}

google.maps.event.addDomListener(window, 'load', initialize);
